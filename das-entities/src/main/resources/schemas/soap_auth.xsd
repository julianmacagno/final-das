<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://das.com/soap_auth"
           targetNamespace="http://das.com/soap_auth" elementFormDefault="qualified">

    <xs:element name="doLoginRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="username" type="xs:string"/>
                <xs:element name="password" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="doLoginResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="token" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

</xs:schema>
